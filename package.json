		"prisma": "^4.16.2",
		"source-map-support": "^0.5.21",
		"supertest": "^6.3.4",
		"ts-jest": "29.1.0",
		"ts-loader": "^9.5.1",
		"ts-node": "^10.9.2",
		"tsconfig-paths": "4.2.0",
		"typescript": "^5.5.2"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "src",
		"testRegex": ".*\\.spec\\.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
